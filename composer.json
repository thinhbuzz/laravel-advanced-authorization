{
  "name": "buzz/laravel-advanced-authorization",
  "description": "Package for handling advanced authorization (role and permission) in Laravel 5.*",
  "keywords": [
    "laravel",
    "laravel5",
    "advanced",
    "authorization",
    "roles",
    "role",
    "permissions",
    "permission",
    "acl"
  ],
  "homepage": "https://github.com/thinhbuzz/laravel-advanced-authorization",
  "license": "MIT",
  "authors": [
    {
      "name": "ThinhBuzz",
      "email": "mr.thinhbuzz@gmail.com",
      "homepage": "https://www.facebook.com/thinh.buzz"
    }
  ],
  "require": {
    "laravel/framework": "5.5.*|5.6.*"
  },
  "autoload": {
    "psr-4": {
      "Buzz\\Authorization\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Models\\": "src/Models/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Buzz\\Authorization\\AuthorizationServiceProvider"
      ]
    }
  }
}